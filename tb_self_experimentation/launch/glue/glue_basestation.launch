<launch>
  	<!-- Visualization part: This include the file for the Rviz and the nodes that publish stuff in the Rviz -->
		<include file = "$(find tb_self_experimentation)/launch/include/rviz_basestation.launch" />	
	
	<!-- Commandline User interface to say when to approach and when to do something else. -->
		<include file = "$(find tb_self_experimentation)/launch/include/approach.launch" />


	<!-- Debugging nodes. Nodes that should run on the turtlebot but we run here so we can debug more quickly -->
		<node name="version_manager" pkg="tb_self_experimentation" type="version_manager.py" output="screen" launch-prefix="xterm -e"/>
	
</launch>
